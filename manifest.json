
{
  "manifest_version": 2,
  "name": "Tab URL Extractor",
  "version": "1.0",
  "description": "Extracts the URL of the active tab",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
